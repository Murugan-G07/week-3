#include <stdio.h>


int main() {
   int a;
 scanf("%d",&a);
 switch (a){
     case 1:
     printf("monday");
     break;
     case 2:
     printf("tuesday");
     break;
     case 3:
     printf("wednesday");
     break;
     case 4:
     printf("thursday");
     break;
     case 5:
     printf("friday");
     break;
     case 6:
     printf("saturday");
     break;
     case 7:
     printf("kryptonday");
     break;
     case 8:
     printf("coluday");
     break;
     case 9:
     printf("daxamday");
     break;
     case 10:
     printf("sunday");
 }
 return 0;
}
