#include <stdio.h>

int main() {
    int a;
    char b;
    scanf("%c %d",&b,&a);
    if(b%2==0 && a%2==0){
        printf("the square is black");
    }
    else if(b%2==1 && a%2==1){
        printf("the square is black");
    }
    else{
        printf("the square is white");
    }
    return 0;
}
